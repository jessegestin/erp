program ERP;

uses
  Vcl.Forms,
  ULogin in 'ULogin.pas' {FRMLogin},
  UMenu in 'UMenu.pas' {frmMenu};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFRMLogin, FRMLogin);
  Application.CreateForm(TfrmMenu, frmMenu);
  Application.Run;
end.
